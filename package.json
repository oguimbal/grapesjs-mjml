{
  "name": "@oguimbal/grapesjs-mjml",
  "version": "0.0.29",
  "description": "MJML Componenets integration in GrapesJS",
  "main": "dist/@oguimbal/grapesjs-mjml.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build-prod": "webpack --env.production",
    "build": "webpack --env.development",
    "start": "webpack-dev-server --open --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oguimbal/grapesjs-mjml.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "mjml",
    "wysiwyg"
  ],
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "grapesjs": "0.x"
  },
  "dependencies": {
    "mjml": "^3.3.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4"
  }
}
